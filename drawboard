MjAyMC0wNC0yOFQxOTowOToxOSswMDowMP1m4YEAAAAldEVYdGRhdGU6bW9kaWZ5ADIwMjAtMDQt
MjhUMTk6MDk6MDMrMDA6MDDk4QbtAAAAAElFTkSuQmCC
ENDSPRITE
;

$main = new MainWindow;
$canvas = $main->Canvas();
$canvas->place(-relx => 0, -rely => 0, -relwidth => 1.0, -relheight => 1.0);
$img = $main->Photo("sprite", -data => $PIECES_SPRITE, -format => "png");

# piece images go here
our %PIECES = (
  K => $main->Photo("K"),
  Q => $main->Photo("Q"),
  B => $main->Photo("B"),
  N => $main->Photo("N"),
  R => $main->Photo("R"),
  P => $main->Photo("P"),
  k => $main->Photo("k"),
  q => $main->Photo("q"),
  b => $main->Photo("b"),
  n => $main->Photo("n"),
  r => $main->Photo("r"),
  p => $main->Photo("p"),
);

# the positions of individual pieces in the sprite
our %PIECEPOS = (
  K => [0, 0, 300, 300],
  Q => [300, 0, 600, 300],
  B => [600, 0, 900, 300],
  N => [900, 0, 1200, 300],
  R => [1200, 0, 1500, 300],
  P => [1500, 0, 1800, 300],
  k => [0, 300, 300, 600],
  q => [300, 300, 600, 600],
  b => [600, 300, 900, 600],
  n => [900, 300, 1200, 600],
  r => [1200, 300, 1500, 600],
  p => [1500, 300, 1800, 600],
);

our $subsample = 0;

sub make_pieces {
  for $key (keys %PIECEPOS) {
    $PIECES{$key}->blank;
    $PIECES{$key}->copy(
      $img, -from => @{$PIECEPOS{$key}},
      -subsample => $subsample, -shrink
    );
    $PIECES{$key}->redither;
  }
}

sub draw {
  # determine the size of the squares
  my $w = $canvas->width;
  my $h = $canvas->height;
  my $m = $w < $h ? $w : $h;
  my $size = int($m / 8);
  if ($size == 0) { return; }                     # prevent division by zero

  # Determine the subsampling rate of the piece images. If it differs
  # from the previous one, resample them.
  my $new_subsample = int(300 / ($size*0.94));
  if ($new_subsample < 1) { $new_subsample = 1; }
  if ($new_subsample != $subsample) {
    $subsample = $new_subsample;
    make_pieces;
  }

  # draw the board
  $canvas->delete("all");
  for ($i = 0; $i < 8; $i++) {
    for ($j = 0; $j < 8; $j++) {
      $canvas->createRectangle(
        $i*$size, $j*$size, ($i+1)*$size, ($j+1)*$size,
        "-fill" => (($i + $j) % 2 == 1) ?
          $COLORS{DARK_SQUARES} : $COLORS{LIGHT_SQUARES}
      );
    }
  }

  # draw the pieces
  my ($i, $j) = 0, 0;
  for (split "", $POSITION) {
    if ($_ eq "/") { $i = 0; $j++; }
    elsif ($_ =~ /[0-9]/) { $i += int($_); }
    elsif ($_ =~ /[KQBNRPkqbnrp]/) {
      $canvas->createImage(
        ($i+0.5)*$size, ($j+0.5)*$size,
        -anchor => "c", -image => $_
      );
      $i++;
    }
  }
}

sub handle_input {
  $line = <STDIN>;
  if ($line) {
    $POSITION = $line;
    draw;
  } else {
    if (eof(STDIN)) {
      close STDIN;
      if (!$LINGER) {
        Tk::exit;
      }
    }
  }
}

$main->bind("<Configure>", \&draw);
$main->aspect(1, 1, 1, 1);
$main->fileevent(STDIN, 'readable', \&handle_input);

$main->MainLoop;

=head1 NAME

drawboard - draw a chess board with the given position

=head1 SYNOPSIS

drawboard [-l]

=head1 OPTIONS

 -l              linger after STDIN is closed

=head1 DESCRIPTION

This program repeatedly reads a chess position in FEN notation from the
standard input and draws a board with this position.

=head1 LICENSE

GPLv3

=head1 ACKNOWLEDGEMENTS

The chess piece images were taken from:
<https://commons.wikimedia.org/wiki/File:Chess_Pieces_Sprite.svg>
and are work of the user Cburnett.

=head1 BUGS

Please report bugs to: <http://gitlab.com/mmj/chesstools>

=cut
